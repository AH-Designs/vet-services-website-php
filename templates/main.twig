      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar" id="icon-bar"></span>
        <span class="icon-bar" id="icon-bar"></span>
        <span class="icon-bar" id="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#"><img class="vets-logo" src="images/vet-services-logo.png"></a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        
            <ul class="nav navbar-nav navbar-right" id="nav-styles">
             
                <li><a href="{{baseUrl()}}">HOME</a></li>
                <li><a href="{{siteUrl('/services')}}">OUR SERVICES</a></li>
                <li><a href="{{siteUrl('/about')}}">ABOUT OUR CLINIC</a></li>
                <li><a href="{{siteUrl('/contact')}}">KEEP IN TOUCH</a></li>
       
            </ul>

    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>

<!--main-content-->
<main>
{% block content %}



{% endblock content %}
</main>


</div><!--End of page wrapper!!!!!!-->



